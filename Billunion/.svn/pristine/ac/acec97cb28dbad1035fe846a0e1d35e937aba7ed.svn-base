#import "StatusCode.h"

@implementation StatusCode

+(NSString *)getErrorStringWithCode:(NSNumber*)code{
 
    NSString *str = nil;
    switch (code.integerValue) {
        case 4062:
             str = @"指定的交易方不存在";
            break;
            
        default:
            break;
    }
    return str;
}

@end
