//我的卖出
- (void)loadMySellDatas:(StockAllType)stockType{
    NSArray *keyArray;
    
    NSString *specifiedTradeName;
    if (self.stockType == StockAllType_MySell_specifiedSell) {
        specifiedTradeName = @"CounterPartyName";
    }else{
        specifiedTradeName = @"CompanyName";
    }
    switch (stockType) {
        case StockAllType_MySell_askSell:
        {
            // 询价卖出
            if ([self.BillType intValue] == 1) {
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"Address",
                            specifiedTradeName,@"haveContract"];
            }else{
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"haveContract"];
            }
         break;
        }
        case StockAllType_MySell_specifiedSell:
        {
            //指定卖出
            if ([self.BillType intValue] == 1) {
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"Address",
                            specifiedTradeName,@"haveContract"];
            }else{
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"haveContract"];
            }
            break;
        }
        case StockAllType_MySell_quoteSell:
        {
            // 报价卖出
            if ([self.BillType intValue] == 1) {
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"Address",
                           @"needContract",@"aimCustomer"];
            }else{
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"needContract",@"aimCustomer"];
            }
            break;
        }
        case StockAllType_MySell_beSpecifiedBuying:
        {
            // 被指定买入
            if ([self.BillType intValue] == 1) {
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"Address",
                            specifiedTradeName,@"needContract"];
            }else{
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"needContract"];
            }
            break;
        }
        default:
            break;
    }

    
    [self setDataInKeyValues:keyArray];
}

