            }else{
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"needContract"];
            }
        }else if(stockType == StockAllType_MyBuying_quoteBuying){
            if ([self.BillType intValue] == 1) {
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"Address",@"haveContract"];
            }else{
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"haveContract"];
            }
        }else{
            if ([self.BillType intValue] == 1) {
                keyArray =@[@"Amount",@"DiscountRate",@"BillType",@"AcceptorIdentityType",
                            @"DueTimeRange",@"TradeStatus",@"ExamineState",@"Address",
